@def APP_MENU_FX_DELAY 350ms;

/**********************************************************************************************************************
 * APP                                                                                                                *
 **********************************************************************************************************************/

#app-root {

    position: relative;
    overflow: hidden;
    max-width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}
#app-head {

    @mixin shadow-head( );
    position: fixed;
    height: 30px;
    left: 0;
    right: 0;
    top: 0;
    z-index: 5;
    margin: 0;
    padding: 15px 0;
    background: WHITE;
}
#app-root.loading > #app-head {

    display: none;
}
html.scroll #app-head {

    position: absolute;
    background-color: BLACK;
}
html.scroll #app-head > .content {

    max-width: 95%;
}
#app-logo {

    @mixin size( 30px, 30px );
    display: block;
    position: relative;
    float: left;
    text-align: center;
    z-index: 1;
    line-height: 30px;
    margin: 0;
    font-size: 24px;
    color: WHITE;
    cursor: pointer;
}
#app-logo > i {

    cursor: pointer;
    background-color: transparent;
}
#app-name {

    display: block;
    position: relative;
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    box-sizing: border-box;
    line-height: 30px;
    width: 100%;
    margin: 0 -30px;
    padding: 0 45px;
    font-size: 18px;
    font-weight: bold;
    color: WHITE;
    cursor: default;
    -webkit-user-select:none;
}
#app-user {

    @mixin size( 30px, 30px );
    display: block;
    position: relative;
    float: right;
    margin: 0;
    background: url('data:image/png;base64,') no-repeat WHITE;
    background-size: contain;
}
#app-body {

    position: absolute;
    width: 100%;
    top: 60px;
    margin: 0;
    padding: 0;
    bottom: 0;
    background: LIGHTEST;
    -webkit-overflow-scrolling: touch;
}
html.scroll #app-body {

    overflow-y: auto;
}
html.scroll #app-root.loading > #app-body,
#app-root.loading > #app-body {

    display: none;
}
#app-root > #app-loading {

    display: none;
}
#app-root.loading > #app-loading {

    display: block;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}
#app-root.loading > #app-loading > .container {

    display: block;
    position: absolute;
    width: 200px;
    height: 148px;
    left: 50%;
    top: 50%;
    margin: -140px 0 0 -100px;
}
#app-root.loading > #app-loading > .container > .logo {

    @mixin size( 50px, 50px );
    display: block;
    position: relative;
    margin: 25px auto;
    padding: 0;
    background: url('data:image/png;base64,') no-repeat WHITE;
    background-size: cover;
    background-repeat: no-repeat;
}
#app-root.loading > #app-loading > .container > .name,
#app-root.loading > #app-loading > .container > .help {

    display: block;
    position: relative;
    text-align: center;
    margin: 0 auto;
    padding: 0;
    line-height: 24px;
    font-size: 22px;
    color: GREY;
    cursor: default;
    -webkit-user-select: none;
}
#app-root.loading > #app-loading > .container > .help {

    font-size: 12px;
    line-height: 24px;
}
#app-view {

    max-width: 1222px;
    margin: auto;
    padding: 0;
}
#app-over {

    @mixin size( 100%, 100% );
    @mixin translate3d( 0, 0, 0 );
    @mixin opacity( 0 );
    @mixin transition( opacity, APP_MENU_FX_DELAY, 0, ease-out );
    position: absolute;
    left: -100%;
    top: 0;
    z-index: 6;
    background-color: rgb(0,0,0);
}
#app-menu {

    @mixin size( 250px, 100% );
    @mixin translate3d( 0, 0, 0 );
    @mixin shadow-menu( );
    @mixin translate3dTransition( APP_MENU_FX_DELAY, 0, ease-out );

    position: absolute;
    min-height: 100%;
    left: -300px;
    top: 0;
    z-index: 10;
    background: WHITE;
}
#app-menu-head {

    @mixin size( 250px,120px );
    position: absolute;
    top: 0;
    left: 0;
    background: GREY;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: 50% 50%;
}
#app-menu-head > div.person-logo {

    @mixin size( 50px, 50px );
    @mixin border-radius( 25px );
    display: block;
    position: relative;
    margin: 20px 180px 0 20px;
    padding: 0;
    float: left;
    background: BLACK;
    background-size: cover;
    background-repeat: no-repeat;
}
#app-menu-head > div.person-name {

    @mixin size( 210px, 25px );
    @mixin caption-line( );
    @mixin text-shadow( );
    display: block;
    position: relative;
    float: left;
    margin: 10px 20px 15px;
    padding: 0;
    font-size: 14px;
    line-height: 25px;
    font-weight: bold;
    color: WHITE;
}
#app-menu-body {

    position: absolute;
    overflow-x: hidden;
    overflow-y: auto;
    top: 120px;
    left: 0;
    right: 0;
    bottom: 0;
    -webkit-overflow-scrolling: touch;
}
#app-menu-wrap {

    position: relative;
    width: 100%;
}
html.default #app-menu-wrap {

    @mixin translate3d( 0, 0, 0 );
}
.content {

    max-width: 740px;
    margin: auto;
    padding: 0;
}
.content > .wrapper {

    display: block;
    position: relative;
    float: left;
    width: 100%;
    margin: 0;
    padding: 10px 0 0;
}
.content > .wrapper .box {

    @mixin shadow-card( );
    display: block;
    position: relative;
    overflow: hidden;
    float: left;
    box-sizing: border-box;
    width: 100%;
    margin: 0 0 10px;
    padding: 0;
    border-radius: 2px;
    background: WHITE;
}

/**********************************************************************************************************************
 * MENU                                                                                                               *
 **********************************************************************************************************************/

/* Menu */
.menu {
    width: 250px;
    min-height: 100%;
    margin: 0;
    padding: 4px 0;
    border: none;
    background: WHITE;
    cursor: default;
}

/* Menu separator */
.menuseparator {
    border-top: 1px solid LIGHT;
    margin: 4px 0;
    padding: 0;
}

/* Menu item */
.menuitem {
    @mixin transition(background-color, 350ms, 0, ease);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: DARK;
    cursor: pointer;
}
.menuitem > div.menuitem-content {
    display: inline-block;
    float: left;
    width: 100%;
}
.menuitem > div.menuitem-content > div.menu-item-content {
    display: inline-block;
    float: left;
    width: 100%;
    margin: 6px 0;
    padding: 0;
}
.menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper,
.menuitem > div.menuitem-content > div.menu-item-content > div.text-wrapper {
    display: inline-block;
    float: left;
    line-height: 30px;
}
.menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon {
    @mixin size(30px,30px);
    @mixin transition(color, 350ms, 0, ease);
    display: inline-block;
    float: left;
    text-align: center;
    color: GREY;
}
.menuitem > div.menuitem-content > div.menu-item-content > div.text-wrapper {
    @mixin size(220px,30px);
    @mixin transition(color, 350ms, 0, ease);
    display: inline-block;
    float: right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 30px;
    color: DARK;
}

/* Menu item - hover state */
.menuitem.menuitem-highlight,
.menuitem.menuitem-hover {
    background-color: LIGHTER;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon {

    color: BLUE;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.home,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.home {

    color: MENU_RED;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.about,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.about {

    color: MENU_RED;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.file,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.file {

    color: MENU_BLUE;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.share,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.share {

    color: MENU_ORANGE;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.contacts,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.contacts {

    color: MENU_GREEN;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.refresh,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.refresh {

    color: MENU_GREEN;
}
.menuitem.menuitem-highlight > div.menuitem-content > div.menu-item-content > div.text-wrapper,
.menuitem.menuitem-hover > div.menuitem-content > div.menu-item-content > div.text-wrapper {

    color: BLACK;
}

/* Menu item - disable state */
.menuitem.menuitem-disabled,
.menuitem.menuitem-disabled > div.menuitem-content > div.menu-item-content > div.text-wrapper {
    color: GREY;
    cursor: default;
}

/**********************************************************************************************************************
 * MEDIA QUERIES                                                                                                      *
 **********************************************************************************************************************/

@media screen and ( max-width: 1260px ){

    #app-view {

        padding: 0;
    }
    .content {

        padding: 0 15px;
    }
}
@media screen and ( max-width: 767px ){
    /* APP */
    #app-head {
        padding: 10px 0;
    }
    #app-logo {
        margin: 0;
    }
    #app-body {
        top: 50px;
    }
    #app-head > .content {

        max-width: 100%;
    }
    /* MENU */
    .menuitem {
        -webkit-transition: none;
           -moz-transition: none;
            -ms-transition: none;
             -o-transition: none;
                transition: none;
    }
    .menuitem > div.menuitem-content > div.menu-item-content {
        margin: 10px 0;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.text-wrapper {
        -webkit-transition: none;
           -moz-transition: none;
            -ms-transition: none;
             -o-transition: none;
                transition: none;
        font-weight: bold;
    }
    .menuitem.menuitem-highlight,
    .menuitem.menuitem-hover {
        background-color: LIGHT;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon {
        color: BLUE;
        -webkit-transition: none;
           -moz-transition: none;
            -ms-transition: none;
             -o-transition: none;
                transition: none;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.home,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.home {
        color: MENU_RED;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.about,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.about {
        color: MENU_RED;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.file,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.file {
        color: MENU_BLUE;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.share,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.share {
        color: MENU_ORANGE;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.contacts,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.contacts {
        color: MENU_GREEN;
    }
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.refresh,
    .menuitem > div.menuitem-content > div.menu-item-content > div.icon-wrapper > i.icon.refresh {
        color: MENU_GREEN;
    }
}