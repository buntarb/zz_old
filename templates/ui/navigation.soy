        <main class="{css mdl-layout__content}">
            <div class="{css page-content}"><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br></div>
        </main>
    </div>
{/template}

/**
 * Scrollable tabs.
 */
{template .scrollableTabs}
    <div class="{css mdl-layout} {css mdl-js-layout} {css mdl-layout--fixed-header}">
        <header class="{css mdl-layout__header}">
            <div class="{css mdl-layout__header-row}">
                <span class="{css mdl-layout-title}">Scrollable tabs</span>
            </div>
            <div class="{css mdl-layout__tab-bar} {css mdl-js-ripple-effect}">
                <a href="#scroll-tab-1" class="{css mdl-layout__tab} {css is-active}">Tab 1</a>
                <a href="#scroll-tab-2" class="{css mdl-layout__tab}">Tab 2</a>
                <a href="#scroll-tab-3" class="{css mdl-layout__tab}">Tab 3</a>
                <a href="#scroll-tab-4" class="{css mdl-layout__tab}">Tab 4</a>
                <a href="#scroll-tab-5" class="{css mdl-layout__tab}">Tab 5</a>
                <a href="#scroll-tab-6" class="{css mdl-layout__tab}">Tab 6</a>
            </div>
        </header>
        <div class="{css mdl-layout__drawer}">
            <span class="{css mdl-layout-title}">Scrollable tabs</span>
        </div>
        <main class="{css mdl-layout__content}">
            <section class="{css mdl-layout__tab-panel} {css is-active}" id="scroll-tab-1">
                <div class="{css page-content}"></div>
            </section>
            <section class="{css mdl-layout__tab-panel}" id="scroll-tab-2">
                <div class="{css page-content}"></div>
            </section>
            <section class="{css mdl-layout__tab-panel}" id="scroll-tab-3">
                <div class="{css page-content}"></div>
            </section>
            <section class="{css mdl-layout__tab-panel}" id="scroll-tab-4">
                <div class="{css page-content}"></div>
            </section>
            <section class="{css mdl-layout__tab-panel}" id="scroll-tab-5">
                <div class="{css page-content}"></div>
            </section>
            <section class="{css mdl-layout__tab-panel}" id="scroll-tab-6">
                <div class="{css page-content}"></div>
            </section>
        </main>
    </div>
{/template}

