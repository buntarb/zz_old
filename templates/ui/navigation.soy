{namespace zz.template.ui.navigation}

/**
 * Default simple layout.
 */
{template .default}
    <div class="{css mdl-layout} {css mdl-js-layout}">
        <header class="{css mdl-layout__header}">
            <div class="{css mdl-layout__header-row}">
                <span class="{css mdl-layout-title}">Default simple layout</span>
                <div class="{css mdl-layout-spacer}"></div>
                <nav class="{css mdl-navigation}">
                    <a class="{css mdl-navigation__link}" href="">Link</a>
                    <a class="{css mdl-navigation__link}" href="">Link</a>
                    <a class="{css mdl-navigation__link}" href="">Link</a>
                    <a class="{css mdl-navigation__link}" href="">Link</a>
                </nav>
            </div>
        </header>
        <div class="{css mdl-layout__drawer}">
            <span class="{css mdl-layout-title}">Title</span>
            <nav class="{css mdl-navigation}">
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
            </nav>
        </div>
        <main class="{css mdl-layout__content}"></main>
    </div>
{/template}

/**
 * No header, fixed drawer.
 */
{template .noHeader}
    <div class="{css mdl-layout} {css mdl-js-layout} {css mdl-layout--fixed-drawer}">
        <div class="{css mdl-layout__drawer}">
            <span class="{css mdl-layout-title}">No header</span>
            <nav class="{css mdl-navigation}">
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
            </nav>
        </div>
        <main class="{css mdl-layout__content}">
            <div class="{css page-content}"></div>
        </main>
    </div>
{/template}

/**
 * Fixed header.
 */
{template .fixedHeader}
    <div class="{css mdl-layout} {css mdl-js-layout} {css mdl-layout--fixed-header}">
        <header class="{css mdl-layout__header}">
        <div class="{css mdl-layout__header-row}">
            <span class="{css mdl-layout-title}">Fixed header</span>
            <div class="{css mdl-layout-spacer}"></div>
            <nav class="{css mdl-navigation} {css mdl-layout--large-screen-only}">
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
            </nav>
        </div>
        </header>
        <div class="{css mdl-layout__drawer}">
            <span class="{css mdl-layout-title}">Title</span>
            <nav class="{css mdl-navigation}">
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
            </nav>
        </div>
        <main class="{css mdl-layout__content}">
            <div class="{css page-content}"></div>
        </main>
    </div>
{/template}

/**
 * Fixed header and drawer
 */
{template .fixedHeaderAndDrawer}
    <div class="{css mdl-layout} {css mdl-js-layout} {css mdl-layout--fixed-drawer} {css mdl-layout--fixed-header}">
        <header class="{css mdl-layout__header}">
            <div class="{css mdl-layout__header-row}">
                <div class="{css mdl-layout-spacer}"></div>
                <div class="{css mdl-textfield} {css mdl-js-textfield} {css mdl-textfield--expandable} {css mdl-textfield--floating-label} {css mdl-textfield--align-right}">
                    <label class="{css mdl-button} {css mdl-js-button} {css mdl-button--icon}" for="fixed-header-drawer-exp">
                        <i class="{css material-icons}">search</i>
                    </label>
                    <div class="{css mdl-textfield__expandable-holder}">
                        <input class="{css mdl-textfield__input}" type="text" name="sample" id="fixed-header-drawer-exp">
                    </div>
                </div>
            </div>
        </header>
        <div class="{css mdl-layout__drawer}">
            <span class="{css mdl-layout-title}">Title</span>
            <nav class="{css mdl-navigation}">
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
                <a class="{css mdl-navigation__link}" href="">Link</a>
            </nav>
        </div>
        <main class="{css mdl-layout__content}">
            <div class="{css page-content}"></div>
        </main>
    </div>
{/template}

